@model IEnumerable<W2022A3PGBNGUYEN.Models.AlbumBaseViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>List of Albums</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
    </tr>
}

</table>
